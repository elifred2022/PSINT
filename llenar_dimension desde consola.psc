Algoritmo llenar_dimension
	
	Dimension nums(10);
	
	Para x<-1 Hasta 10 Con Paso 1 Hacer
		
		Escribir "escribir numero para la posicion 1: " x;
		
		Leer nums(x);
		
	Fin Para
	
	Escribir "el contenido del areglo es: ";
	
	Para i<-1 Hasta 10 Con Paso 1 Hacer
		
		Escribir nums(i);
		
	Fin Para
	
FinAlgoritmo
